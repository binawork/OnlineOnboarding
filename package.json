{
  "name": "onlineonboarding",
  "version": "1.0.0",
  "description": "Online Onboarding system",
  "main": "index.js",
  "scripts": {
    "dev": "webpack --mode development ./onboarding/src/index.js --output ./onboarding/static/js/main.js",
    "start": "webpack --mode development --watch ./onboarding/src/index.js --output ./onboarding/static/js/main.js",
    "build": "webpack --mode production ./onboarding/src/index.js --output ./onboarding/static/js/main.js",
    "dev2": "webpack --mode development ./onboarding/src/employee.js --output ./onboarding/static/js/employee.js --config ./webpack.config.employee.js",
    "start2": "webpack --mode development --watch ./onboarding/src/employee.js --output ./onboarding/static/js/employee.js --config ./webpack.config.employee.js",
    "build_employee": "webpack --mode production ./onboarding/src/employee.js --output ./onboarding/static/js/employee.js --config ./webpack.config.employee.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/binawork/OnlineOnboarding.git"
  },
  "keywords": [
    "onboarding"
  ],
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/binawork/OnlineOnboarding/issues"
  },
  "homepage": "https://github.com/binawork/OnlineOnboarding#readme",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.11.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "bootstrap": "^4.5.2",
    "html-react-parser": "^0.14.2",
    "jquery": "^3.6.1",
    "netlify": "^4.8.0",
    "node-sass": "^8.0.0",
    "npm": "^9.1.2",
    "popper.js": "^1.16.1",
    "react": "^16.13.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.3.4",
    "react-scripts": "5.0.1",
    "regenerator-runtime": "^0.13.11",
    "summernote": "^0.8.18",
    "uuidv4": "^6.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.3.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "file-loader": "^6.1.1",
    "html-webpack-plugin": "^4.5.0",
